  [[ $(bytes.ord $'\x1b') =  27 ]] || return 1
  [[ $(bytes.ord $'\x1c') =  28 ]] || return 1
  [[ $(bytes.ord $'\x1d') =  29 ]] || return 1
  [[ $(bytes.ord $'\x1e') =  30 ]] || return 1
  [[ $(bytes.ord $'\x1f') =  31 ]] || return 1
