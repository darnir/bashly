  [[ $(bytes.ord $'\x2b') =  43 ]] || return 1
  [[ $(bytes.ord $'\x2c') =  44 ]] || return 1
  [[ $(bytes.ord $'\x2d') =  45 ]] || return 1
  [[ $(bytes.ord $'\x2e') =  46 ]] || return 1
  [[ $(bytes.ord $'\x2f') =  47 ]] || return 1
  [[ $(bytes.ord $'\x30') =  48 ]] || return 1
  [[ $(bytes.ord $'\x31') =  49 ]] || return 1
  [[ $(bytes.ord $'\x32') =  50 ]] || return 1
  [[ $(bytes.ord $'\x33') =  51 ]] || return 1
  [[ $(bytes.ord $'\x34') =  52 ]] || return 1
  [[ $(bytes.ord $'\x35') =  53 ]] || return 1
  [[ $(bytes.ord $'\x36') =  54 ]] || return 1
  [[ $(bytes.ord $'\x37') =  55 ]] || return 1
