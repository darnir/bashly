#!/bin/bash

: load bytes

function test {
  typeset newline

  [[ $(bytes.ord $'\x01') =   1 ]] || return 1
  [[ $(bytes.ord $'\x02') =   2 ]] || return 1
  [[ $(bytes.ord $'\x03') =   3 ]] || return 1
  [[ $(bytes.ord $'\x04') =   4 ]] || return 1
  [[ $(bytes.ord $'\x05') =   5 ]] || return 1
  [[ $(bytes.ord $'\x06') =   6 ]] || return 1
  [[ $(bytes.ord $'\x07') =   7 ]] || return 1
  [[ $(bytes.ord $'\x08') =   8 ]] || return 1
  [[ $(bytes.ord $'\x09') =   9 ]] || return 1

  IFS= read -rd '' newline <  <(bytes.ord $'\x0a')
  [[ $newline = 10 ]] || return 1

  [[ $(bytes.ord $'\x0b') =  11 ]] || return 1
  [[ $(bytes.ord $'\x0c') =  12 ]] || return 1
  [[ $(bytes.ord $'\x0d') =  13 ]] || return 1
  [[ $(bytes.ord $'\x0e') =  14 ]] || return 1
  [[ $(bytes.ord $'\x0f') =  15 ]] || return 1
  [[ $(bytes.ord $'\x10') =  16 ]] || return 1
  [[ $(bytes.ord $'\x11') =  17 ]] || return 1
  [[ $(bytes.ord $'\x12') =  18 ]] || return 1
  [[ $(bytes.ord $'\x13') =  19 ]] || return 1
  [[ $(bytes.ord $'\x14') =  20 ]] || return 1
  [[ $(bytes.ord $'\x15') =  21 ]] || return 1
  [[ $(bytes.ord $'\x16') =  22 ]] || return 1
  [[ $(bytes.ord $'\x17') =  23 ]] || return 1
